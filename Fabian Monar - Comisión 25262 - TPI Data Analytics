ntas_clean[\"cantidad\"]\n","\n","# Agrupaci√≥n por categor√≠a con m√©tricas\n","resumen_categoria = (\n","    ventas_clean.groupby(\"categoria\", as_index=False)\n","    .agg({\n","        \"ingreso\": \"sum\",\n","        \"cantidad\": \"sum\",\n","        \"precio\": \"mean\",\n","        \"id_venta\": \"count\"\n","    })\n","    .rename(columns={\n","        \"ingreso\": \"ingreso_total\",\n","        \"cantidad\": \"unidades_totales\",\n","        \"precio\": \"precio_promedio\",\n","        \"id_venta\": \"ventas_totales\"\n","    })\n",")\n","\n","# Ticket promedio\n","resumen_categoria[\"ticket_promedio\"] = (\n","    resumen_categoria[\"ingreso_total\"] / resumen_categoria[\"ventas_totales\"]\n",")\n","\n","# Ordenar por ingreso total\n","resumen_categoria = resumen_categoria.sort_values(\n","    by=\"ingreso_total\", ascending=False\n",")\n","\n","print(\"üìä Resumen por categor√≠a (ordenado por ingreso total):\")\n","display(resumen_categoria)\n","\n","# ============================================\n","# üìà Gr√°fico: Ingreso total por categor√≠a\n","# ============================================\n","\n","import matplotlib.pyplot as plt\n","\n","plt.figure(figsize=(10, 5))\n","plt.bar(resumen_categoria[\"categoria\"], resumen_categoria[\"ingreso_total\"])\n","plt.title(\"Ingreso Total por Categor√≠a\")\n","plt.xlabel(\"Categor√≠a\")\n","plt.ylabel(\"Ingreso Total\")\n","plt.xticks(rotation=45)\n","plt.tight_layout()\n","plt.show()\n","\n","print(\"\"\"\n","üìù Resumen del Punto 8:\n","- Se realiz√≥ la agregaci√≥n por categor√≠a.\n","- Se calcularon ingreso total, unidades, precio promedio y ticket promedio.\n","- Se cre√≥ el gr√°fico de ingreso total por categor√≠a.\n","\"\"\")\n"],"metadata":{"colab":{"base_uri":"https://localhost:8080/","height":754},"id":"ZAnmEz4styhX","executionInfo":{"status":"ok","timestamp":1763771262862,"user_tz":180,"elapsed":491,"user":{"displayName":"fabian monar","userId":"11890366176889281631"}},"outputId":"9b914398-f7f7-