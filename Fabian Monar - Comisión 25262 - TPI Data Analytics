\n","\n","# ============================================================\n","# üßπ LIMPIEZA INICIAL (duplicados + nulos)\n","# ============================================================\n","\n","print(\"\\n=== LIMPIEZA INICIAL ===\")\n","\n","# --- Duplicados ---\n","print(\"Duplicados en ventas:\", ventas.duplicated().sum())\n","ventas = ventas.drop_duplicates()\n","\n","print(\"Duplicados en clientes:\", clientes.duplicated().sum())\n","clientes = clientes.drop_duplicates()\n","\n","print(\"Duplicados en marketing:\", marketing.duplicated().sum())\n","marketing = marketing.drop_duplicates()\n","\n","print(\"‚úîÔ∏è Duplicados eliminados correctamente.\")\n","\n","\n","# --- Valores nulos ---\n","print(\"\\nValores nulos en ventas:\")\n","print(ventas.isna().sum())\n","\n","print(\"\\nValores nulos en clientes:\")\n","print(clientes.isna().sum())\n","\n","print(\"\\nValores nulos en marketing:\")\n","print(marketing.isna().sum())\n","\n","\n","# ============================================================\n","# üìà VISUALIZACIONES DEL EDA\n","# ============================================================\n","\n","def graficos(df, nombre):\n","    num_cols = df.select_dtypes(include=['float64','int64']).columns\n","\n","    print(f\"\\nüìä Gr√°ficos para {nombre}\")\n","\n","    # Histogramas\n","    df[num_cols].hist(figsize=(12,6))\n","    plt.suptitle(f\"Histogramas ‚Äî {nombre}\")\n","    plt.show()\n","\n","    # Boxplots\n","    plt.figure(figsize=(12,6))\n","    sns.boxplot(data=df[num_cols])\n","    plt.title(f\"Boxplots ‚Äî {nombre}\")\n","    plt.show()\n","\n","    # Correlaci√≥n\n","    plt.figure(figsize=(10,6))\n","    sns.heatmap(df[num_cols].corr(), annot=True, cmap=\"Blues\")\n","    plt.title(f\"Matriz de correlaci√≥n ‚Äî {nombre}\")\n","    plt.show()\n","\n","\n","graficos(ventas, \"Ventas\")\n","graficos(clientes, \"Clientes\")\n","graficos(marketing, \"Marketing\")\n","\n","\n","# ============================================================